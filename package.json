{
  "name": "video-quickstart-js",
  "version": "1.0.0-dev",
  "description": "Twilio Video SDK Quick Start for JavaScript",
  "main": "index.js",
  "scripts": {
    "clean:quickstart": "rimraf quickstart/public",
    "start": "node server",
    "quickstart": "npm run clean:quickstart && npm run build:quickstart && node server",
    "prequickstart:dev": "npm run clean:quickstart ",
    "quickstart:dev": "parcel watch -d ./quickstart/public --public-url ./ quickstart/src/*.html "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio/video-quickstart-js.git"
  },
  "keywords": [
    "twilio",
    "video",
    "chat",
    "ip",
    "real",
    "time",
    "diggity"
  ],
  "author": "Twilio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twilio/video-quickstart-js/issues"
  },
  "homepage": "https://github.com/twilio/video-quickstart-js#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "prismjs": "^1.6.0",
    "stackblur-canvas": "^1.4.0",
    "twilio": "^3.0.0-rc.16",
    "twilio-video": "^1.14.1"
  },
  "devDependencies": {
    "@types/twilio-video": "^2.0.4",
    "browserify": "^14.3.0",
    "copyfiles": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.1",
    "sass": "^1.17.0",
    "typescript": "^3.3.3"
  }
}
